{
  "name": "redux-promise-dispatch",
  "version": "1.4.1",
  "description": "Simplifying promises with redux and redux-thunk",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kjrocker/redux-promise-dispatch"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "npm test -- -w",
    "init": "mkdir dist",
    "clean": "rimraf -rf dist",
    "prebuild": "npm run clean && npm run init",
    "build": "npm run build:commonjs && npm run build:es",
    "pretest": "npm run build",
    "test": "mocha --compilers js:babel-core/register ./src/**/*.test.js",
    "test:single": "mocha --compilers js:babel-core/register",
    "build:commonjs": "rimraf lib && tsc --target es5 --outDir lib --module commonjs --declaration",
    "build:es": "rimraf es && tsc --target es6 --outDir es --module es6 --declaration",
    "ts": "ts-node src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "redux",
    "redux-thunk",
    "promises"
  ],
  "peerDependencies": {
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "mocha": "^3.1.2",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.3.0",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.1.2",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2"
  },
  "files": [
    "es",
    "lib"
  ],
  "author": "Kevin Rocker",
  "license": "MIT",
  "types": "./lib/index.d.ts",
  "jsnext:main": "./es/index.js",
  "module": "./es/index.js"
}
